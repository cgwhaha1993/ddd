/*清楚默认样式*/
html,body,p,h1,h2,h3,h4,h5,ul{
    margin: 0;
    padding: 0;
} 
h1,h2,h3,h4,h5{
    font-weight: normal;
} 
img{
    border:0 none;
}
ul{list-style: none;} 
a{text-decoration: none;}


/*公用样式*/
.g_fl{
    float: left;
}
.g_fr{
    float: right;
}
.g_clear:after{
    content: "";
    height: 0;
    visibility: hidden;
    clear:both;
    display:block;
    overflow: hidden;
}
.g_clear{
    zoom: 1;
}
